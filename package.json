{
  "name": "quizzing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "dev:link-vars": "ln -sf .env.local .dev.vars",
    "worker:build": "node build-worker.js",
    "worker:watch": "node build-worker.js --watch",
    "worker:dev": "pnpm dev:link-vars && concurrently -k -n BUILD,WRANGLER -c yellow,cyan \"pnpm worker:watch\" \"wrangler dev dist-worker/handler.mjs\"",
    "worker:deploy": "wrangler deploy dist-worker/handler.mjs",
    "worker:all": "pnpm worker:build && pnpm worker:deploy"
  },
  "dependencies": {
    "@clerk/clerk-js": "^3.12.0",
    "@formkit/auto-animate": "^1.0.0-beta.1",
    "@headlessui/vue": "^1.7.8",
    "@supabase/supabase-js": "^1.35.3",
    "@types/d3": "^7.4.0",
    "@vueuse/head": "^2.0.0",
    "@vueuse/rxjs": "^13.2.0",
    "d3": "^7.8.0",
    "dayjs": "^1.11.2",
    "dexie": "^3.2.2",
    "dexie-export-import": "^1.0.3",
    "dexie-observable": "^3.0.0-beta.11",
    "dotenv": "^16.0.1",
    "downloadjs": "^1.4.7",
    "motion": "^10.8.1",
    "nanoid": "^4.0.0",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primevue": "^3",
    "radash": "^10.6.0",
    "randomcolor": "^0.6.2",
    "rxjs": "^7.5.5",
    "stretchy": "^2.0.1",
    "unsplash-js": "^7.0.15",
    "vue": "^3.5.13",
    "vue-router": "4",
    "vue-slicksort": "^2.0.0-alpha.5",
    "worktop": "^0.8.0-next.14"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.11.0",
    "@total-typescript/ts-reset": "^0.3.7",
    "@types/downloadjs": "^1.4.3",
    "@types/node": "^22.15.0",
    "@types/randomcolor": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "concurrently": "^9.1.2",
    "esbuild": "^0.14.39",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.22.0",
    "postcss": "8",
    "postcss-html": "^1.4.1",
    "prettier": "^2.7.1",
    "sass": "^1.51.0",
    "stylelint": "^14.8.5",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^3.4.1",
    "vue-tsc": "^2.2.10",
    "wrangler": "^4.14.4"
  }
}
